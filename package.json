{
  "name": "boostify",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "format": "prettier . --write --ignore-path .prettierignore",
